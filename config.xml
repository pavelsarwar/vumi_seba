<?xml version="1.0" encoding="UTF-8"?>

<!-- config.xml reference: https://build.phonegap.com/docs/config-xml -->
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.vumi.seba"
		versionCode = "27" 
        version   = "1.0.0">

    <name>ভূমি সেবা</name>

    <description>
		ভূমি সেবা: সেবাগ্রহীতার জ্ঞাতব্য ও করণীয়


    </description>

    <author href="http://phonegap.com" email="support@phonegap.com">
       Pavel Sarwar
    </author>
	


      
	  
	  
	<icon src="res/mipmap-ldpi/ic_launcher.png" platform="android" qualifier="ldpi" />
	<icon src="res/mipmap-mdpi/ic_launcher.png" platform="android" qualifier="mdpi" />
	<icon src="res/mipmap-hdpi/ic_launcher.png" platform="android" qualifier="hdpi" />
	<icon src="res/mipmap-xhdpi/ic_launcher.png" platform="android" qualifier="xhdpi" />
	<icon src="res/mipmap-xxhdpi/ic_launcher.png" platform="android" qualifier="xxhdpi" />
	<icon src="res/fr-mipmap-xxxhdpi/ic_launcher.png" platform="android" qualifier="fr-xxxhdpi" />
	




    
    <preference name="permissions"                value="none"/>
	
	<gap:splash density="ldpi" platform="android" src="res/screen/android/screen-ldpi-portrait.png" />
	<gap:splash density="mdpi" platform="android" src="res/screen/android/screen-mdpi-portrait.png" />
	<gap:splash density="hdpi" platform="android" src="res/screen/android/screen-hdpi-portrait.png" />
	<gap:splash density="xhdpi" platform="android" src="res/screen/android/screen-xhdpi-portrait.png" />
	<gap:splash density="xxhdpi" platform="android" src="res/screen/android/screen-xxhdpi-portrait.png" />
	<gap:splash density="xxxhdpi" platform="android" src="res/screen/android/screen-xxxhdpi-portrait.png" />
	
	
	
	






    <!-- Customize your app and platform with the preference element. -->
	<preference name="permissions" value="none"/>
    <preference name="phonegap-version"           value="cli-5.2.0" />          <!-- all: current version of PhoneGap -->
    <preference name="orientation"                value="default" />        <!-- all: default means both landscape and portrait are enabled -->
    <preference name="target-device"              value="universal" />      <!-- all: possible values handset, tablet, or universal -->
    <preference name="fullscreen"                 value="false" />           <!-- all: hides the status bar at the top of the screen -->
	<preference name="android-minSdkVersion" value="14" /> <!-- android: MIN SDK version supported on the target device. MAX version is blank by default. -->
	<preference name="android-installLocation" value="auto" /> <!-- android: app install location. 'auto' will choose. 'internalOnly' is device memory. 'preferExternal' is SDCard. -->

    <access origin="*"/>
    
</widget>