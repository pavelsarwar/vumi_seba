<?xml version="1.0" encoding="UTF-8"?>

<!-- config.xml reference: https://build.phonegap.com/docs/config-xml -->
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.vumi.seba"
		versionCode = "36" 
        version   = "1.1.8">

    <name>ভূমি সেবা</name>

    <description>
		ভূমি সেবা: সেবাগ্রহীতার জ্ঞাতব্য ও করণীয়


    </description>

    <author href="http://phonegap.com" email="support@phonegap.com">
       Pavel Sarwar
    </author>
	


      
	  
	  
	<icon src="res/mipmap-ldpi/ic_launcher.png" gap:platform="android" gap:qualifier="mdpi" />
	<icon src="res/mipmap-mdpi/ic_launcher.png" gap:platform="android" gap:qualifier="mdpi" />
	<icon src="res/mipmap-hdpi/ic_launcher.png" gap:platform="android" gap:qualifier="hdpi" />
	<icon src="res/mipmap-xhdpi/ic_launcher.png" gap:platform="android" gap:qualifier="xhdpi" />
	<icon src="res/mipmap-xxhdpi/ic_launcher.png" gap:platform="android" gap:qualifier="xxhdpi" />
	<icon src="res/mipmap-xxxhdpi/ic_launcher.png" gap:platform="android" gap:qualifier="xxxhdpi" />
	




    
    <preference name="permissions"                value="none"/>
	
	<gap:splash gap:density="ldpi" gap:platform="android" src="res/screen/android/screen-ldpi-portrait.png" />
	<gap:splash gap:density="mdpi" gap:platform="android" src="res/screen/android/screen-mdpi-portrait.png" />
	<gap:splash gap:density="hdpi" gap:platform="android" src="res/screen/android/screen-hdpi-portrait.png" />
	<gap:splash gap:density="xhdpi" gap:platform="android" src="res/screen/android/screen-xhdpi-portrait.png" />
	<gap:splash gap:density="xxhdpi" gap:platform="android" src="res/screen/android/screen-xxhdpi-portrait.png" />
	<gap:splash gap:density="xxxhdpi" gap:platform="android" src="res/screen/android/screen-xxxhdpi-portrait.png" />
	
	






    <!-- Customize your app and platform with the preference element. -->
	<preference name="permissions" value="none"/>
    <preference name="phonegap-version"           value="cli-8.1.1" />           <!-- all: current version of PhoneGap -->
    <preference name="orientation"                value="default" />        <!-- all: default means both landscape and portrait are enabled -->
    <preference name="target-device"              value="universal" />      <!-- all: possible values handset, tablet, or universal -->
    <preference name="fullscreen"                 value="false" />           <!-- all: hides the status bar at the top of the screen -->
	<preference name="android-minSdkVersion" value="26" /> <!-- android: MIN SDK version supported on the target device. MAX version is blank by default. -->
	<preference name="android-installLocation" value="auto" /> <!-- android: app install location. 'auto' will choose. 'internalOnly' is device memory. 'preferExternal' is SDCard. -->

  <plugin name="cordova-plugin-whitelist"           source="npm" spec="~1.2.1" />

  <access origin="https://form.jotform.me/60268259401454*" />
  <allow-navigation href="https://form.jotform.me/*"/>
  
  <allow-intent href="https://form.jotform.me/60268259401454*"/>
    
</widget>